{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base"
    ],
    "prHourlyLimit": 5,
    "labels": ["update"],
    "packageRules": [
        {
            "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
            "automerge": true,
            "automergeType": "pr",
            "platformAutomerge": true
        }
    ]
}
